{
  "build": {
    "beforeDevCommand": "sf dev --silent",
    "beforeBuildCommand": "sf pack -o dist",
    "devPath": "http://localhost:8080",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "wds",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "path": {
        "all": true
      },
      "dialog": {
        "all": false,
        "ask": false,
        "confirm": false,
        "message": false,
        "open": true,
        "save": true
      },
      "protocol": {
        "all": true,
        "asset": true,
        "assetScope": [
          "$APPDATA/**/*",
          "$RESOURCE/**/*"
        ]
      },
      "fs": {
        "all": false,
        "copyFile": false,
        "createDir": false,
        "exists": false,
        "readDir": false,
        "readFile": false,
        "removeDir": false,
        "removeFile": false,
        "renameFile": false,
        "scope": [],
        "writeFile": false
      },
      "window": {
        "all": false,
        "center": false,
        "close": false,
        "create": false,
        "hide": false,
        "maximize": false,
        "minimize": false,
        "print": false,
        "requestUserAttention": false,
        "setAlwaysOnTop": true,
        "setCursorGrab": false,
        "setCursorIcon": false,
        "setCursorPosition": true,
        "setCursorVisible": false,
        "setDecorations": false,
        "setFocus": true,
        "setFullscreen": true,
        "setIcon": false,
        "setIgnoreCursorEvents": false,
        "setMaxSize": false,
        "setMinSize": false,
        "setPosition": false,
        "setResizable": false,
        "setSize": false,
        "setSkipTaskbar": false,
        "setTitle": false,
        "show": false,
        "startDragging": false,
        "unmaximize": false,
        "unminimize": false
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/icon.icns",
        "icons/icon.ico",
        "icons/icon_16x16.png",
        "icons/icon_32x32.png",
        "icons/icon_48x48.png",
        "icons/icon_128x128.png",
        "icons/icon_256x256.png"
      ],
      "identifier": "wds",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [
        "../assets/*"
      ],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "systemTray": null,
    "windows": [
      {
        "title": "wds",
        "fullscreen": false,
        "resizable": true,
        "width": 600,
        "height": 600,
        "minWidth": 600,
        "minHeight": 600,
        "visible": false,
        "transparent": true
      }
    ]
  }
}